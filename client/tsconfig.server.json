{
  //"extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "dist",
    "noEmit": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./"
  },
  "include": ["src/server.ts"],
  "exclude": ["node_modules"]
}